{
  "name": "wd-parent-base-ui",
  "version": "1.0.1",
  "description": "",
  "author": "WeAre Dev S.A.S.",
  "keywords": [],
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "npx serve ./dist",
    "prebuild": "rimraf dist",
    "build": "vite build && cp -r ./staticwebapp.config.json ./dist",
    "dist": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "eject": "vite eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs --fix",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "prune": "ts-prune",
    "clean": "rimraf node_modules package-lock.json yarn.lock dist covered",
    "sonar": "sonar-scanner",
    "sonar-debug": "sonar-scanner -X",
    "prepare": "husky install",
    "docker": "docker build -t coe-wd-parent-base-ui . && docker run -p 3000:3000 coe-wd-parent-base-ui"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write ."
    ]
  },
  "dependencies": {
